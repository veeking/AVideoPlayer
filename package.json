{
  "name": "alpha-video-player",
  "version": "0.1.0",
  "description": "A alpha video player base on WebGL render draw. ",
  "type": "module",
  "main": "lib/alpha-video-player.cjs.js",
  "module": "lib/alpha-video-player.esm.js",
  "browser": "lib/alpha-video-player.umd.js",
  "exports": {
    "default": "./src/index.js",
    "require": "./lib/alpha-video-player.umd.js",
    "import": "./lib/alpha-video-player.esm.js",
    "node": "./lib/alpha-video-player.cjs.js"
  },
  "scripts": {
    "dev": "NODE_ENV=development rollup -c -w",
    "build": "NODE_ENV=production rollup -c --environment BUILD:production",
    "deploy": "npm run build && npm publish --registry http://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veeking/AVideoPlayer.git"
  },
  "keywords": [
    "video",
    "webgl"
  ],
  "engines": {
    "node": ">=10.12"
  },
  "author": "veeking",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/veeking/AVideoPlayer/issues"
  },
  "homepage": "https://github.com/veeking/AVideoPlayer#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rollup": "^2.52.3",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-glslify": "^1.2.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
